{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "paths": {
      "@german/*": ["./models/german/*"],
      "@data/*": ["./data/*"]
    },
    "resolveJsonModule": true,
    "rootDir": "./src",
    "sourceMap": false,
    "target": "es6"
  },
  "lib": ["es6"],
  "exclude": ["node_modules", "coverage", "**.spec.ts"],
  "include": ["src"]
}
