{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "paths": {
      "german/*": ["src/models/german/*"]
    },
    "resolveJsonModule": true,
    "rootDir": "./src",
    "sourceMap": false,
    "target": "es6"
  },
  "lib": ["es6"],
  "exclude": ["node_modules"]
}
