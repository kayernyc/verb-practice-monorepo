{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "composite": false,
    "esModuleInterop": true,
    "module": "node16",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "paths": {
      "@controllers/*": ["./controllers/*"],
      "@models/*": ["./models/*"],
      "@germanHydrationFunctions/*": ["./models/german/hydrationFunctions/*.ts"],
      "@germanUtilities/*": ["./models/german/utilities/*.ts"],
      "@tilities/*": ["./utilities/*"],
    }
  },
  "include": ["./src"]
}
